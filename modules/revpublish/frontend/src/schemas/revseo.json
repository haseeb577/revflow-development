{
  "module": {
    "id": "revseo",
    "name": "RevSEO Intelligence",
    "number": 4,
    "suite": "Lead Generation",
    "version": "1.0.0"
  },
  "meta": {
    "title": "RevSEO Intelligence Dashboard",
