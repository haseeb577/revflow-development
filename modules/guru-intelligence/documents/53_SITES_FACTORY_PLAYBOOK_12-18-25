üéØ 1. REAL DATA SOURCE FOR LOCATIONS
Using DataForSEO Location Database (Already Have Access!)
# First, let's see what DataForSEO offers for location data
cat > /tmp/test_dataforseo_locations.py << 'EOF'
#!/usr/bin/env python3
import os
import requests
import base64
import json

login = "shimon.abouzaglo@vsrcouncil.org"
password = "0dde0c3934e10d5e"

credentials = base64.b64encode(f"{login}:{password}".encode()).decode()
headers = {
    'Authorization': f'Basic {credentials}',
    'Content-Type': 'application/json'
}

# Get location data
url = "https://api.dataforseo.com/v3/serp/google/locations"
response = requests.get(url, headers=headers)
data = response.json()

print("DataForSEO Locations API:")
print(json.dumps(data['tasks'][0]['result'][:5], indent=2))
EOF

